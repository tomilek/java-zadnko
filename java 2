var a = 'fff 32'
var b = '79 ggg'

console.log(typeof a ,a )
console.log(typeof b ,b )

a = parseInt(a)
b = parseInt(b)

console.log(typeof a, a,)
console.log(typeof b, b)


console.log(isNaN(a))
console.log(isNaN(b))

console.log(a+b)

