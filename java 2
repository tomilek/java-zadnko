var a = '023.2324'
var b = '54.34244'

console.log(typeof a , a)
console.log(typeof b,  b)

a = parseFloat(a)
b = parseFloat(b)

c = a + b 

c = c.toFixed(2)

console.log(typeof c,  c)
console.log(c)

